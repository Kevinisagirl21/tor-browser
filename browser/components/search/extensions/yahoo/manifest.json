{
  "name": "Yahoo",
  "description": "Yahoo Search",
  "manifest_version": 2,
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "yahoo@search.mozilla.org"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.ico"
  },
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Yahoo",
      "search_url": "https://search.yahoo.com/yhs/search",
      "search_form": "https://search.yahoo.com/yhs/search?p={searchTerms}&ei=UTF-8&hspart=mozilla",
      "search_url_get_params": "p={searchTerms}&ei=UTF-8&hspart=mozilla",
      "params": [
        {
          "name": "hsimp",
          "condition": "purpose",
          "purpose": "searchbar",
          "value": "yhs-001"
        },
        {
          "name": "hsimp",
          "condition": "purpose",
          "purpose": "keyword",
          "value": "yhs-002"
        },
        {
          "name": "hsimp",
          "condition": "purpose",
          "purpose": "homepage",
          "value": "yhs-003"
        },
        {
          "name": "hsimp",
          "condition": "purpose",
          "purpose": "newtab",
          "value": "yhs-004"
        },
        {
          "name": "hsimp",
          "condition": "purpose",
          "purpose": "contextmenu",
          "value": "yhs-005"
        }
      ],
      "suggest_url": "https://search.yahoo.com/sugg/ff",
      "suggest_url_get_params": "output=fxjson&appid=ffd&command={searchTerms}"
    }
  }
}